<div class="one gradi-1">
  <span id="about-link" style="position:absolute;top:-45px;"></span>
  <h1>About Nate</h1>

  <div class="row">
  </br>
    <div class="small-12 large-2 columns show-desktop">
      <div class="nav-buffer">&nbsp;</div>
    </div>
    <div class="small-12 large-6 columns">
      <% @abouts.each do |about| %>
        <%= markdown(about.body) %>
    </div>
    <div class="small-12 large-4 columns">
      <div class="nav-buffer show-desktop">&nbsp;</div>
      <%= image_tag about.image.url(:large), class: "show-desktop" %>
      <%= image_tag about.image.url(:medium), class: "show-mobile" %>
      <% end %>
    </div>
  </div>
</div>

