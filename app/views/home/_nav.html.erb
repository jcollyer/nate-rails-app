<% if current_user %>
<div id="user">
  <div class="row">
    <div class="small-12 columns">
      <h1>Logged in as <%= current_user.email %></h1>
    </div>
  </div>
  <div class="row">
    <div class="small-12 large-4 columns">
      <h3>&nbsp;</h3>
    </div>
    <div class="small-12 large-4 columns">
      <h3>
        <%= link_to "Teachings", teachings_path %>
        <%= link_to "Biblebooks", biblebooks_path %>
        <%= link_to "Bio", "/bios/1/edit" %>
        <%= link_to "Books", books_path %>
        <%= link_to "Calvary", "/notes/1/edit" %>
        <%= link_to "About", "/abouts/1/edit" %>
        </br>
        </br>
        <%= link_to "Log out", log_out_path %>
      </h3>
    </div>
    <div class="small-12 large-4 columns">
      <h3>&nbsp;</h3>
    </div>
  </div>
</div>
<% else %>
  <div id="before-nav" class="show-desktop">&nbsp;</div>
  <nav id="main-nav" class="old-nav show-desktop">
    <div class="logo old-logo show-desktop">&nbsp;</div>
    <ul>
      <li>teachings</li>
      <li>books</li>
      <li>social</li>
      <li>about</li>
      <li>calvary</li>
    </ul>
  </nav>
  <nav id="main-nav-mobile" class="show-mobile">
    <div class="logo old-logo show-desktop">&nbsp;</div>
      <li><div id="hamburger" class="fa fa-bars fa-2x">&nbsp;</div></li>
    <ul id="mobile-links">
      <li>teachings</li>
      <li>books</li>
      <li>social</li>
      <li>about</li>
      <li>calvary</li>
    </ul>
  </nav>

<% end %>
