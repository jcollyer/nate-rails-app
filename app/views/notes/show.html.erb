<p id="notice"><%= notice %></p>
<div class="row">
  <div class="small-12 large-8 columns">
    <%=  markdown(@note.body) %>
  </div>
  <div class="small-12 large-4 columns">
    <h3><a href="<%= @note.link %>">Link One</a></h3>
    <h3><a href="<%= @note.linktwo %>">Link Two</a></h3>
  </div>
</div>

<div class="row">
  <div class="small-8 columns">
    <h3><%= link_to 'Edit', edit_note_path(@note) %></h3>
    <h3><%= link_to 'Cancel', root_url %></h3>
  </div>
</div>

