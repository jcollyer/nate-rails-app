<div class="row">
  <div class="small-12 columns">
    <h1>Log in</h1>
  </div>
<%= form_tag sessions_path do %>
  <div class="large-6 small-12 columns">
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="large-6 small-12 columns">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </div>
  <div class="small-12 columns">
    <%= submit_tag "Log in" %>
  </div>
<% end %>
</div>
